{
  "compilerOptions": {
    "outDir": "dist",
    "sourceMap": true,
    "target": "esnext",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "src",
    "typeRoots": ["./typings", "./node_modules/@types"],
    "types": ["express", "jest"]
  },
  "include": ["./*.js", "src/**/*.ts", "test/**/*.ts"]
}
